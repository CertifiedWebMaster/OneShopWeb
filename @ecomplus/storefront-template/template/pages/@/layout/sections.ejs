<%
// load page sections and render each one
const { sections } = _.cms(_.pageName)
if (Array.isArray(sections)) {
  for (let i = 0; i < sections.length; i++) {
    const opt = sections[i]
    if (opt) {
      %>
      <%- await include(`/@/sections/${opt.type}`, { _, opt }) %>
      <%
    }
  }
}
%>
