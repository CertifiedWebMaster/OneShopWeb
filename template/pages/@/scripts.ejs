<%
// merge store data to expose on window object
const { store, categories, brands, items } = _
delete store.description
const storeData = { store, categories, brands, items }
%>

<script>
  window.$settings = <%- JSON.stringify(_.settings) %>
  window.$data = <%- JSON.stringify(storeData) %>
  window.$context = <%- JSON.stringify(await _.resolveRoute()) %>
</script>
<script src="/storefront.js"></script>
