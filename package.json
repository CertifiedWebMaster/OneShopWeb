    "sass": "^1.25.0",
    "sass-loader": "^8.0.2",
    "standard": "^14.3.1",
    "standard-version": "^7.0.1",
    "vue-template-compiler": "^2.6.11"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
