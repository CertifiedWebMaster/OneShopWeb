  "homepage": "https://github.com/ecomclub/storefront-app#readme",
  "peerDependencies": {
    "@ecomplus/utils": "^1.2.0",
    "core-js": "3.x"
  },
  "dependencies": {
    "@ecomplus/client": "^2.0.2",
    "@ecomplus/i18n": "^1.3.1",
    "@ecomplus/passport-client": "^1.0.6",
